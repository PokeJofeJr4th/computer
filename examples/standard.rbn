fn print(string_location) {
    write(1, 18);
    while (*string_location != 0) {
        write(*string_location, 0);
        string_location += 1;
        yield();
    };
    write(0, 0);
    yield();
}

const hello_world = "Hello, World!";
const wololo = "Wololo!!";

fn main() {
    print(&hello_world);
    print(&wololo);
}
